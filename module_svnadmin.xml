  <target name="compile.module.svnadmin.production" depends="register.custom.compilers" description="Compile module svnadmin; production classes">
    <mkdir dir="${svnadmin.output.dir}"/>
    <javac2 destdir="${svnadmin.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.svnadmin}/javac">
      <compilerarg line="${compiler.args.svnadmin}"/>
      <bootclasspath refid="svnadmin.module.bootclasspath"/>
      <classpath refid="svnadmin.module.production.classpath"/>
      <src refid="svnadmin.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.svnadmin"/>
    </javac2>
    
    <copy todir="${svnadmin.output.dir}">
      <fileset dir="${module.svnadmin.basedir}/src/main/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.svnadmin.basedir}/src/main/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.svnadmin.tests" depends="register.custom.compilers,compile.module.svnadmin.production" description="compile module svnadmin; test classes" unless="skip.tests">
    <mkdir dir="${svnadmin.testoutput.dir}"/>
    <javac2 destdir="${svnadmin.testoutput.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.svnadmin}/javac">
      <compilerarg line="${compiler.args.svnadmin}"/>
